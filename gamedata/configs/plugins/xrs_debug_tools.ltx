
[plugin]
enable = true

developer_debug = true					; Enables dev_debug and sim_debug
debug_gulag	= false						; Prints gulag info

keybind_print_position	= P				; Keybind to print actor position into file save position
keybind_toggle_top_hud = 2				; Main menu keybind to cycle through available hud displays
keybind_toggle_left_hud = 1
keybind_toggle_right_hud = 3
keybind_teleport_to_cam = T				; Keybind to teleport to last demo_record cam position
keybind_debug_ui_nearest_stalker = F1	; Keybind within Debug UI to grab near NPC's logic data and fill editboxes
keybind_debug_ui_spawn_position = F2	; Keybind within Debug UI to set spawn position at actor's current location

file_save_position_info = positional_info.txt	; File to write print_position when keypressed in main menu
