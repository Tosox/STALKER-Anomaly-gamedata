; Наследуемся от класса O_PHYS_S для получения возможности работать с cse_abstract (se_item.script, class_register.script) для изменение direction объекта
; Список оружия сгенерирован python-парсером (dyn_obj.py) в папке с дополнениями
[awr_physic_object]
GroupControlSection 			= spawn_group
$spawn        					= "physics\object"
$prefetch       				= 8
class      			 			= O_PHYS_S
remove_time     				= 60
script_binding      			= bind_physic_object.init

[awr_tiski]:awr_physic_object
visual                     		= dynamics\workshop_room\awr_tiski.ogf
script_binding					= bind_physic_object.init
fixed_bones						= link
